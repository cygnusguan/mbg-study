                            res.end(
                                errorTemplate ||
                                    "Error occurred loading redirectUrl"
                            );
                            reject(
                                NodeAuthError.createUnableToLoadRedirectUrlError()
                            );
                            return;
                        } else if (url === CommonConstants.FORWARD_SLASH) {
                            res.end(
                                successTemplate ||
                                    "Auth code was successfully acquired. You can close this window now."
                            );
                            return;
