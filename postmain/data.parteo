var runSync = function() {
  xhr = new XMLHttpRequest();

  xhr.onreadystatechange = function() {
    if (this.readyState == 4) {
      assert.equal("Hello World", this.responseText);
      sys.puts("done");
    }
  };
  xhr.open("GET", url, false);
  xhr.send();
}
