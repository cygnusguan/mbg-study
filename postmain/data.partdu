      // (do a pretty print to see this)
      function recreateavlt () {
        avlt = new AVLTree();

        [10, 5, 3, 8, 15, 12, 37].forEach(function (k) {
          avlt.insert(k, 'some ' + k);
        });

        avlt.getNumberOfKeys().should.equal(7);
