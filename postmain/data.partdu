      checkRemoved(firstSet, [3, 4, 10]);

      avlt.delete(15);   // Node with only one child (on the right)
      avlt.checkIsAVLT();
      checkRemoved(firstSet, [3, 4, 10, 15]);

      // Second set: some rebalancing necessary
