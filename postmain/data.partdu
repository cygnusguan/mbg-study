    done = after(2, done)

    function cb(err) {
        assert.equal(err.message, 1);
        done()
    }

    var next = after(3, cb, function(err) {
        assert.equal(err.message, 2)
        done()
    });

    next()
    next(new Error(1))
    next(new Error(2))
})

